{
	"name": "backfill-bsky",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"backfill": "bun --env-file=.env backfill/main.ts",
		"after-backfill": "bun --env-file=.env scripts/afterBackfill.ts",
		"buffer": "deno run --env-file=.env -A buffer/main.ts",
		"buffer:ingest": "deno run --v8-flags=--max-old-space-size=65536 --env-file=.env -A buffer/ingest.ts",
		"request-crawl": "bun --env-file=.env scripts/requestCrawlPdses.ts",
		"index-repos": "bun --env-file=.env scripts/indexRepos.ts",
		"create-indexes": "bun --env-file=.env scripts/createIndexes.ts",
		"drop-indexes": "bun --env-file=.env scripts/dropIndexes.ts",
		"fetch-dids": "bun --env-file=.env scripts/fetchDids.ts"
	},
	"keywords": [],
	"author": "",
	"license": "MPL-2.0",
	"devDependencies": {
		"@types/bun": "^1.1.14",
		"@types/node": "^22.9.4",
		"@types/pg": "^8.11.10",
		"dprint": "^0.47.5",
